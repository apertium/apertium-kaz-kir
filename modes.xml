  <mode name="kaz-kir" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="kaz-kir.automorf.bin"/>
      </program>
      <program name="cg-proc  -n -w -1">
        <file name="kaz-kir.rlx.bin"/>
      </program>
